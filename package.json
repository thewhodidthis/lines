{
  "name": "multipolar",
  "version": "0.0.7",
  "description": "Helps produce common geometric forms",
  "keywords": [
    "math",
    "geometry"
  ],
  "homepage": "https://github.com/thewhodidthis/multipolar#readme",
  "bugs": {
    "url": "https://github.com/thewhodidthis/multipolar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/multipolar.git"
  },
  "license": "ISC",
  "author": "Sotiri Bakagiannis <sotirios.bakagiannis@gmail.com> (http://thewhodidthis.com)",
  "main": "index",
  "module": "index.mjs",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "preexample": "eslint example/index.js",
    "example": "cd example; rollup -c node:x -f iife -o bundle.js index.js",
    "prestart": "eslint index.mjs",
    "start": "rollup -c node:x -f cjs -e @thewhodidthis/arithmetics,poltocar index.mjs -o index.js --no-interop --preferConst",
    "pretest": "eslint test.js",
    "test": "node test"
  },
  "eslintConfig": {
    "extends": "@thewhodidthis"
  },
  "dependencies": {
    "@thewhodidthis/arithmetics": "^0.0.7",
    "poltocar": "^2.0.7"
  },
  "devDependencies": {
    "@thewhodidthis/eslint-config": "^0.1.7",
    "eslint": "^7.3.1",
    "rollup": "^2.18.0",
    "rollup-config-x": "^3.0.0",
    "tapeless": "^1.0.3"
  },
  "engines": {
    "node": ">=6.11.0",
    "npm": ">=3.10.10"
  }
}
