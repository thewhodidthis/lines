{
  "name": "multipolar",
  "version": "0.0.1",
  "description": "Helps produce common geometric forms",
  "main": "index",
  "module": "index.mjs",
  "homepage": "https://github.com/thewhodidthis/multipolar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/multipolar.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/multipolar/issues"
  },
  "scripts": {
    "pretest": "eslint test.js",
    "test": "node test",
    "prestart": "eslint index.mjs",
    "start": "rollup -c node:x -e @thewhodidthis/arithmetics,poltocar -f cjs --no-interop --environment PREFER_CONST index.mjs > index.js",
    "preexample": "eslint example/index.js",
    "example": "cd example; rollup -c node:x -f iife index.js | cpp -P | grep -v '^//' | uniq > bundle.js"
  },
  "eslintConfig": {
    "extends": "@thewhodidthis"
  },
  "engines": {
    "node": ">=6.11.0",
    "npm": ">=3.10.10"
  },
  "dependencies": {
    "@thewhodidthis/arithmetics": "github:thewhodidthis/arithmetics",
    "poltocar": "github:thewhodidthis/poltocar"
  },
  "devDependencies": {
    "@thewhodidthis/eslint-config": "latest",
    "eslint": "latest",
    "rollup": "latest",
    "rollup-config-x": "latest",
    "tapeless": "latest"
  },
  "keywords": [
    "math",
    "geometry"
  ],
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}
